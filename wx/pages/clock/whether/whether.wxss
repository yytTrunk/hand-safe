/* pages/clock/clock.wxss */

.content{
  margin: 30rpx 30rpx;
}
.list{
  height: 80rpx;
  border-bottom: 1rpx solid #dfe5e9;
  margin-bottom: 30rpx;
  display: flex;
  justify-content: space-between;
}
.list input{
  height: 80rpx;
  width: 85%;
}
.list text{
  line-height: 80rpx;
}
.list image{
  width: 20rpx;
  margin-top: 30rpx;
}
.picker{
  line-height: 80rpx;
  width: 100%;
}
picker{
  width: 94%;
  display: block;
}
.hint{
  width: 100%;
  background-color:  #f1f5f8;
  height: 64rpx;
}
.hint text{
  color: #c4c7ca;
  line-height: 64rpx;
  margin-left: 30rpx;
}